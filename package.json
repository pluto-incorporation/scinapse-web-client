{
  "name": "scinapse",
  "version": "1.0.0",
  "description": "Academic search engine 'scinapse' web client repository",
  "sideEffects": false,
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p start:build dev start:server",
    "start:server": "nodemon ./dist/server/main.js",
    "start:build": "webpack --config webpack.local.dev.config --watch",
    "clean": "rm -rf dist && rm -rf dist-sls && rm -rf temp && rm -f dist.zip && mkdir dist",
    "build-sls": "tsc -p scripts",
    "pack": "cd dist && zip -rq ../dist.zip . ** && cd .. && ls dist",
    "build:dev": "npm run clean && npm run build-sls && node --max_old_space_size=3000 dist-sls/build/dev && cp scripts/package.json dist/package.json && cd dist && npm i && cd ..",
    "build:prod": "npm run clean && npm run build-sls && NODE_ENV=production node --max_old_space_size=4000 dist-sls/build/prod && cp scripts/package.json dist/package.json && cd dist && npm i && cd ..",
    "deploy:dev": "npm run build:dev && npm run pack && serverless deploy -s dev",
    "deploy:stage": "npm run build:prod && npm run pack && serverless deploy -s stage",
    "deploy:prod": "npm run deploy:stage && STAGE=stage npm run test:e2e && serverless deploy -s production",
    "dev": "webpack-dev-server",
    "test": "jest --coverage",
    "test:ci": "jest --coverage --maxWorkers=2",
    "test:e2e": "jest -c jest.e2e.config.js --runInBand",
    "pretty": "prettier --write \"app/**/*.{ts,tsx,scss}\"",
    "precommit": "lint-staged"
  },
