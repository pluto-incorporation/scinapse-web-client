{
  "name": "pluto-web-client",
  "version": "1.0.0",
  "description": "Pluto web client",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p start:server start:build dev",
    "start:server": "nodemon ./dist/bundle.js --watch \"./dist\"",
    "start:build": "webpack --config webpack.local.dev.config --watch",
    "build": "webpack",
    "clean": "rm -rf dist && rm -rf dist-sls && rm -rf temp && rm -f dist.zip && mkdir dist",
    "build:stage": "npm run clean && webpack --config webpack.stage.server.config && webpack --config webpack.stage.browser.config",
    "build:prod": "npm run clean && webpack --config webpack.prod.server.config && webpack --config webpack.prod.browser.config",
    "build-sls": "tsc -p scripts",
    "deploy": "node dist-sls/deploy/deploy.js",
    "test:e2e": "nightwatch",
    "pack": "cp version dist/version && cp scripts/package.json dist/package.json && cd dist && npm install --production && zip -q -r ../dist.zip . ** && cd ..",
    "deploy:stage": "npm run build:stage && npm run build-sls && NODE_ENV=stage npm run deploy && npm run pack && serverless deploy -s stage",
    "deploy:prod": "npm run build:prod && npm run build-sls && NODE_ENV=production npm run deploy && npm run pack && serverless deploy -s prod",
    "dev": "webpack-dev-server",
    "ssr-test": "npm run clean && webpack --config webpack.stage-server.config && node dist/bundle",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pluto-net/web-client.git"
  },
  "author": "Pluto",
  "license": "GNU",
  "bugs": {
    "url": "https://github.com/pluto-net/web-client/issues"
  },
  "homepage": "https://github.com/pluto-net/web-client#readme",
  "devDependencies": {
    "@types/autosize": "^3.0.6",
    "@types/classnames": "^2.2.3",
    "@types/enzyme": "^3.1.6",
    "@types/express": "^4.11.1",
    "@types/history": "^4.6.2",
    "@types/jest": "^22.0.1",
    "@types/lodash": "^4.14.92",
    "@types/material-ui": "^0.20.5",
    "@types/node": "^8.5.9",
    "@types/prop-types": "^15.5.2",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "@types/react-ga": "^2.3.0",
    "@types/react-helmet": "^5.0.4",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/react-router-redux": "^5.0.11",
    "@types/redux-logger": "^3.0.5",
    "@types/redux-mock-store": "0.0.11",
    "@types/toastr": "^2.1.35",
    "@types/ua-parser-js": "^0.7.32",
    "autoprefixer": "^7.2.5",
    "awesome-typescript-loader": "^3.4.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chromedriver": "2.35.0",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "isomorphic-style-loader": "^4.0.0",
    "jasmine-reporters": "^2.3.0",
    "jest": "^22.1.2",
    "lodash-webpack-plugin": "^0.11.5",
    "markdown-loader": "^2.0.2",
    "nightwatch": "^0.9.19",
    "node-sass": "^4.7.2",
    "nodemon": "^1.15.1",
    "npm-run-all": "^4.1.2",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.10",
    "precss": "^2.0.0",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.1",
    "s3": "^4.4.0",
    "sass-loader": "^6.0.6",
    "selenium-server": "^3.11.0",
    "serverless": "^1.26.1",
    "serverless-lambda-version": "^0.1.2",
    "svg-sprite-loader": "^3.6.2",
    "ts-jest": "^22.0.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.8.0",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^2.11.0",
    "webpack-jarvis": "^0.2.2"
  },
  "dependencies": {
    "autosize": "^4.0.0",
    "aws-sdk": "^2.220.1",
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "isbot": "^2.1.2",
    "lodash": "^4.17.5",
    "material-ui": "^0.20.0",
    "moment": "^2.20.1",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "raven-js": "^3.22.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-ga": "^2.4.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "store": "^2.0.12",
    "toastr": "^2.1.4",
    "typed-immutable-record": "0.0.6",
    "ua-parser-js": "^0.7.17"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/jest/jestPreprocessor.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest/jestReporter.js",
    "verbose": true,
    "rootDir": "",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageDirectory": "output/coverage",
    "testRegex": "__tests__/.*_spec.tsx$",
    "unmockedModulePathPatterns": [
      "/.*\\.scss$",
      "<rootDir>/app/__mocks__",
      "<rootDir>/app/__tests__"
    ],
    "setupFiles": [
      "<rootDir>/app/__tests__/preload.tsx"
    ]
  }
}
