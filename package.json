{
  "name": "pluto-web-client",
  "version": "1.0.0",
  "description": "Pluto web client",
  "sideEffects": false,
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p start:build dev start:server",
    "start:server": "nodemon ./dist/server/main.js --watch \"./dist\"",
    "start:build": "webpack --config webpack.local.dev.config --watch",
    "clean": "rm -rf dist && rm -rf dist-sls && rm -rf temp && rm -f dist.zip && mkdir dist",
    "build-sls": "tsc -p scripts",
    "pack": "cd dist && zip -r ../dist.zip . ** && cd .. && ls dist",
    "build:dev": "npm run clean && npm run build-sls && node --max_old_space_size=2048 dist-sls/build/dev && cp scripts/package.json dist/package.json && cd dist && npm i && cd ..",
    "deploy:dev": "npm run build:dev && npm run pack && serverless deploy -s dev",
    "build:prod": "npm run clean && npm run build-sls && NODE_ENV=production node --max_old_space_size=2048 dist-sls/build/prod",
    "deploy:prod": "npm run build:prod && npm run pack && serverless deploy -s production",
    "dev": "webpack-dev-server",
    "test": "jest --coverage",
    "pretty": "prettier --write \"app/**/*.{ts,tsx,scss}\"",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pluto-net/web-client.git"
  },
  "author": "Pluto",
  "license": "GNU",
